{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "my-mcp-server",
	"main": "src/index.ts",
	"compatibility_date": "2025-03-10",
	"compatibility_flags": ["nodejs_compat"],

	"durable_objects": {
		"bindings": [
			{
				"class_name": "MyMCP",
				"name": "MCP_OBJECT"
			}
		]
	},

	"migrations": [
		{
			"tag": "v1",
			"new_classes": ["MyMCP"]
		}
	],

	"observability": { "enabled": true },

	"vars": {
		"NOTION_PAGE_ID": "2bf0e7783a1d4bf0bde9b793b284683f",
		"NOTION_SCHEDULE_DB": "56dce8f4c45649888e5a1cf7bfa60d62"
	},

	"ai": {
		"binding": "AI",
		"remote": true
	}, 

	
